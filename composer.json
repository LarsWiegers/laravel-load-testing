{
    "name": "larswiegers/laravel-load-testing",
    "description": "Load test your application using the best tools for the job, in a Laravel style.",
    "keywords": [
        "larswiegers",
        "laravel-load-testing"
    ],
    "homepage": "https://github.com/larswiegers/laravel-load-testing",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Lars Wiegers",
            "email": "larswiegers@live.nl",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Larswiegers\\LaravelLoadTesting\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Larswiegers\\LaravelLoadTesting\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Larswiegers\\LaravelLoadTesting\\LaravelLoadTestingServiceProvider"
            ],
            "aliases": {
                "LaravelLoadTesting": "Larswiegers\\LaravelLoadTesting\\LaravelLoadTestingFacade"
            }
        }
    }
}
